{
  "name": "portfolio",
  "description": "Antonio Almena's Portfolio extended from https://github.com/ueno-llc/ueno-gatsby-starter",
  "version": "1.2.11",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "gatsby-starter",
    "ueno",
    "portfolio",
    "9ntonio"
  ],
  "contributors": [
    "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
    "Jeremy Barbet <jeremgraph@gmail.com>",
    "Antonio Almena <antonio@spacebase.one>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/9ntonio/portfolio"
  },
  "scripts": {
    "dev": "gatsby --verbose develop",
    "build": "gatsby --verbose build",
    "start": "gatsby serve --port ${PORT:-3000}",
    "clean": "rimraf .cache public",
    "lint": "yarn prettier && yarn tslint && yarn stylelint",
    "tslint": "tslint --fix 'src/**/*.{ts,tsx}' --project tsconfig.json",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "prettier": "prettier --check '**/*.{ts,tsx,js,jsx,json,md}'",
    "prettier-fix": "prettier --write '**/*.{ts,tsx,js,jsx,json,md}'",
    "stylelint-fix": "stylelint 'src/**/*.scss' --fix",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "if",
            "else",
            "mixin",
            "each",
            "include",
            "function",
            "return",
            "warn",
            "for"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "dependencies": {
    "gatsby": "2.24.62",
    "gatsby-cli": "^2.12.82",
    "gatsby-image": "2.4.19",
    "gatsby-plugin-config": "1.0.7",
    "gatsby-plugin-layout": "1.3.11",
    "gatsby-plugin-manifest": "2.4.30",
    "gatsby-plugin-netlify": "2.3.15",
    "gatsby-plugin-offline": "3.2.28",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.11",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-robots-txt": "1.5.2",
    "gatsby-plugin-sharp": "^2.6.26",
    "gatsby-plugin-sitemap": "2.4.13",
    "gatsby-plugin-typescript": "2.4.20",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-graphql-universal": "3.4.0",
    "gatsby-transformer-sharp": "2.5.15",
    "gsap": "3.5.1",
    "gsap-tools": "1.0.12",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-html-parser": "2.0.2",
    "react-player": "2.6.2",
    "renovate": "23.30.2"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@types/gsap": "1.20.2",
    "@types/lodash": "4.14.161",
    "@types/node": "14.10.3",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.6",
    "@types/react-helmet": "5.0.15",
    "classnames": "2.2.6",
    "classnames-loader": "2.1.0",
    "node-sass": "4.14.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "stylelint": "13.7.1",
    "stylelint-config-recommended": "3.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0",
    "typescript": "4.0.2"
  }
}
